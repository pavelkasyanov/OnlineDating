<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.onlinedating.model">

    <class name="ContactList" table="ContactList">
        <id name="contactlist_id" type="java.lang.Integer">
            <column name="ContactListID" />
            <generator class="identity">
            </generator>
        </id>
        <set name="contactsSet" table="Contacts" fetch="select" >
            <key>
                <column name="ContactList_ContactListID" not-null="true"></column>
            </key>
            <one-to-many class="Contacts"/>
        </set>

    </class>
</hibernate-mapping>